            <ProgressBar
                android:id="@+id/pro29"
                style="@style/Widget.ProgressBar.Vertical"
                android:layout_width="@dimen/_10sdp"
                android:layout_height="match_parent"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_weight="1" />

            <ProgressBar
                android:id="@+id/pro30"
                style="@style/Widget.ProgressBar.Vertical"
                android:layout_width="@dimen/_10sdp"
                android:layout_height="match_parent"
